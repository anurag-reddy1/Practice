
public class PowerOfTwo {
	
	public static boolean find(int n) {
		if(n==0)
			return false;
		if(n==1)
			return true;
		while(n!=1) {
			if(n%2!=0)
				return false;
			else
			n=n/2;
		}	
		return true;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=16;
		System.out.println(16&15);
		//System.out.println(find(n));
	}

}
