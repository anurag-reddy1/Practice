

public class SingleNumber {

	public static int find(int[] nums) {
		int a = 0;
	    for (int i : nums) {
	      a ^= i;
	      System.out.println(a);
	    }
	    return a;
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] nums= {4,1,3,1,3};
		System.out.println(find(nums));

	}

}
